@model Net.UI.Models.DTO.EditDto
@{
}





<h1 class="mt-3">Edit User</h1>

@if (Model is not null)

{

	<form method="post" asp-controller="AppUser" asp-action="EditUser">


		<div class="mt-3">

			<label class="form-label">Email</label>
			<input type="text" class="form-control" asp-for="Email" readonly />


		</div>


		<div class="mt-3">

			<label class="form-label">Name</label>
			<input type="text" class="form-control" asp-for="Name" readonly />


		</div>



		<div class="mt-3">

			<label class="form-label">Last</label>
			<input type="text" class="form-control" asp-for="LastName" readonly />


		</div>


		<div class="mt-3">

			<label class="form-label">Bio "Optional" </label>
			<input type="text" class="form-control" asp-for="Bio" />


		</div>


		<div class="mt-3 d-flex justify-content-between">

			<button type="submit" class="btn btn-primary">Save</button>


		</div>




	</form>




	<form method="post" asp-controller="AppUser" asp-action="EditAdress">


		<div class="mt-3">

			<label class="form-label">AdressOne</label>
			<input type="text" class="form-control" asp-for="AdressOne" />


		</div>


		<div class="mt-3">

			<label class="form-label">AdressTwo</label>
			<input type="text" class="form-control" asp-for="AdressTwo" />


		</div>



		<div class="mt-3">

			<label class="form-label">PostalCode</label>
			<input type="text" class="form-control" asp-for="PostalCode" />


		</div>


		<div class="mt-3">

			<label class="form-label">City </label>
			<input type="text" class="form-control" asp-for="City" />


		</div>


		<div class="mt-3 d-flex justify-content-between">

			<button type="submit" class="btn btn-primary">Save</button>


		</div>




	</form>






}









